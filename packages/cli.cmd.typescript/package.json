{
  "name": "@osdk/cli.cmd.typescript",
  "private": true,
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/js/index.mjs",
      "require": "./build/js/index.cjs"
    },
    "./*": {
      "types": "./build/types/public/*.d.ts",
      "import": "./build/js/public/*.mjs",
      "require": "./build/js/public/*.cjs"
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "transpile": "tsup",
    "typecheck": "tsc-absolute"
  },
  "dependencies": {
    "@arethetypeswrong/cli": "^0.15.2",
    "@osdk/cli.common": "workspace:^",
    "@osdk/gateway": "workspace:^",
    "@osdk/generator": "workspace:^",
    "@osdk/shared.net": "workspace:^",
    "consola": "^3.2.3",
    "find-up": "^7.0.0",
    "tslib": "^2.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/yargs": "^17.0.29",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "osdk-unstable-typescript": "./bin/osdk-unstable-typescript.mjs"
  },
  "files": [
    "build/types",
    "build/js",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/js/index.cjs",
  "module": "./build/js/index.mjs",
  "types": "./build/types/index.d.ts",
  "type": "module"
}
