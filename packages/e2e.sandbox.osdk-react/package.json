{
  "name": "@osdk/e2e.sandbox.osdk-react",
  "private": true,
  "version": "3.6.0-beta.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "rm -rf src/generatedNoCheck/* && osdk-unstable-typescript generate --outDir src/generatedNoCheck --ontologyPath ontology.json --version dev --internal --packageType module",
    "dev": "vite",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check --config $(find-up dprint.json)",
    "preview": "vite preview",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~",
    "@osdk/oauth": "workspace:~",
    "@osdk/react": "workspace:~",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tiny-invariant": "^1.3.3"
  },
  "peerDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~"
  },
  "devDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/cli.cmd.typescript": "workspace:~",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.8.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.43.0",
    "vite": "^6.3.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "type": "module"
}
