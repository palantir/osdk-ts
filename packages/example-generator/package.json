{
  "name": "@osdk/example-generator",
  "private": true,
  "version": "0.16.0-beta.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "generate": "./bin/exampleGenerator.mjs ../../examples",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run --pool=forks",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@osdk/create-app": "workspace:~",
    "@osdk/create-widget": "workspace:~",
    "consola": "^3.4.2",
    "dir-compare": "^5.0.0",
    "find-up": "^7.0.0",
    "globby": "^14.1.0",
    "jest-diff": "^29.7.0",
    "tmp": "^0.2.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@types/node": "^18.19.124",
    "@types/tmp": "^0.2.6",
    "@types/yargs": "^17.0.33",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "example-generator": "./bin/exampleGenerator.mjs"
  },
  "files": [],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "type": "module"
}
