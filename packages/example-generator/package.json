{
  "name": "@osdk/example-generator",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/js/index.mjs",
      "require": "./build/js/index.cjs"
    },
    "./*": {
      "types": "./build/types/public/*.d.ts",
      "import": "./build/js/public/*.mjs",
      "require": "./build/js/public/*.cjs"
    }
  },
  "scripts": {
    "check": "./bin/exampleGenerator.mjs ../../examples --check",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "generate": "./bin/exampleGenerator.mjs ../../examples",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "transpile": "tsup",
    "typecheck": "tsc-absolute"
  },
  "dependencies": {
    "@osdk/create-app": "workspace:*",
    "consola": "^3.2.3",
    "dir-compare": "^4.2.0",
    "find-up": "^7.0.0",
    "globby": "^14.0.1",
    "tmp": "^0.2.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tmp": "^0.2.6",
    "@types/yargs": "^17.0.29",
    "typescript": "^5.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "example-generator": "./bin/exampleGenerator.mjs"
  },
  "files": [
    "build/types",
    "build/js",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/js/index.cjs",
  "module": "./build/js/index.mjs",
  "types": "./build/types/index.d.ts",
  "type": "module"
}
