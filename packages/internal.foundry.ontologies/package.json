{
  "name": "@osdk/internal.foundry.ontologies",
  "version": "0.1.0-beta.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": "./build/esm/index.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "../../scripts/build_common/check-attw.sh esm",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "transpile": "find . \\( -path build/cjs -or -path build/esm -or -path build/browser \\) -type f \\( -name '*.js' -or -name '*.js.map' -or -name '*.cjs' -or -name '*.cjs.map' \\) -delete && tsup",
    "typecheck": "find . \\( -path build/cjs -or -path build/esm -or -path build/browser \\) -type f \\( -name '*.ts' -or -name '*.ts.map' -or -name '*.cts' -or -name '*.cts.map' \\) -delete && ../../scripts/build_common/typecheck.sh esm"
  },
  "dependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/internal.foundry.core": "workspace:*",
    "@osdk/shared.client": "workspace:~",
    "@osdk/shared.net": "workspace:~",
    "@osdk/shared.net.platformapi": "workspace:~"
  },
  "devDependencies": {
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/monorepo.tsup": "workspace:~",
    "typescript": "^5.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/js/index.cjs",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "sls": {
    "dependencies": {
      "com.palantir.foundry.api:api-gateway": {
        "minVersion": "1.866.0",
        "maxVersion": "1.x.x"
      }
    }
  },
  "type": "module"
}
