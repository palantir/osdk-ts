{
  "name": "@osdk/client",
  "version": "0.19.0",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "browser": "./build/browser/index.browser.js"
    },
    "./*": {
      "import": "./build/esm/public/*.js",
      "browser": "./build/browser/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "../../scripts/build_common/check-attw.sh esm",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "node ./generateMockOntology.js",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run --pool=forks",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "../../scripts/build_common/typecheck.sh esm"
  },
  "dependencies": {
    "@osdk/api": "workspace:^",
    "@osdk/generator-converters": "workspace:^",
    "@osdk/shared.net": "workspace:^",
    "conjure-lite": "^0.4.4",
    "fast-deep-equal": "^3.1.3",
    "fetch-retry": "^6.0.0",
    "isomorphic-ws": "^5.0.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.17.0"
  },
  "peerDependencies": {
    "pino": "^9.0.0"
  },
  "devDependencies": {
    "@osdk/generator": "workspace:^",
    "@osdk/internal.foundry": "workspace:^",
    "@osdk/shared.test": "workspace:^",
    "@types/geojson": "^7946.0.14",
    "@types/ws": "^8.5.10",
    "jest-extended": "^4.0.2",
    "p-defer": "^4.0.1",
    "p-state": "^2.0.1",
    "pino": "^9.0.0",
    "ts-expect": "^1.3.0",
    "type-fest": "^4.17.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "sls": {
    "dependencies": {
      "com.palantir.foundry.api:api-gateway": {
        "minVersion": "1.834.0",
        "maxVersion": "1.x.x"
      }
    }
  },
  "type": "module"
}
