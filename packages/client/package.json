{
  "name": "@osdk/client",
  "version": "0.3.0",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/js/index.mjs",
      "require": "./build/js/index.cjs"
    },
    "./*": {
      "types": "./build/types/public/*.d.ts",
      "import": "./build/js/public/*.mjs",
      "require": "./build/js/public/*.cjs"
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "node ./generateMockOntology.js",
    "dev:transpile": "tsup --watch",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run --pool=forks",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "tsc-absolute --build"
  },
  "dependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/gateway": "workspace:*",
    "@osdk/shared.net": "workspace:*",
    "conjure-lite": "^0.3.1",
    "fetch-retry": "^5.0.6",
    "isomorphic-ws": "^5.0.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@osdk/generator": "workspace:*",
    "@osdk/shared.test": "workspace:*",
    "@types/ws": "^8.5.10",
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "files": [
    "build/types",
    "build/js",
    "CHANGELOG.md",
    "package.json",
    "*.d.ts"
  ],
  "main": "./build/js/index.cjs",
  "module": "./build/js/index.mjs",
  "types": "./build/types/index.d.ts",
  "type": "module"
}
