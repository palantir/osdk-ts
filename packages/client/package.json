{
  "name": "@osdk/client",
  "version": "0.20.0-beta.0",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": "./build/esm/index.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "../../scripts/build_common/check-attw.sh esm",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run --pool=forks",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "../../scripts/build_common/typecheck.sh esm"
  },
  "dependencies": {
    "@osdk/api": "workspace:^",
    "@osdk/client.api": "workspace:*",
    "@osdk/generator-converters": "workspace:^",
    "@osdk/shared.client": "workspace:^",
    "@osdk/shared.client.impl": "workspace:^",
    "@osdk/shared.net.errors": "workspace:^",
    "@osdk/shared.net.fetch": "workspace:^",
    "@types/geojson": "^7946.0.14",
    "conjure-lite": "^0.4.4",
    "fast-deep-equal": "^3.1.3",
    "fetch-retry": "^6.0.0",
    "isomorphic-ws": "^5.0.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.17.0"
  },
  "peerDependencies": {
    "pino": "^9.1.0"
  },
  "peerDependenciesMeta": {
    "pino": {
      "optional": true
    }
  },
  "devDependencies": {
    "@osdk/client.test.ontology": "workspace:^",
    "@osdk/internal.foundry": "workspace:^",
    "@osdk/shared.test": "workspace:^",
    "@types/geojson": "^7946.0.14",
    "@types/ws": "^8.5.10",
    "jest-extended": "^4.0.2",
    "msw": "^2.3.0",
    "p-defer": "^4.0.1",
    "p-state": "^2.0.1",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "ts-expect": "^1.3.0",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "sls": {
    "dependencies": {
      "com.palantir.foundry.api:api-gateway": {
        "minVersion": "1.834.0",
        "maxVersion": "1.x.x"
      }
    }
  },
  "type": "module"
}
