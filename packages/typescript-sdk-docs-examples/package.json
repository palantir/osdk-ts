{
  "name": "@osdk/typescript-sdk-docs-examples",
  "private": true,
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "scripts": {
    "build": "pnpm run codegen && pnpm run generateExamples && pnpm run transpileEsm",
    "clean": "rm -rf dist src/generatedNoCheck examples src/examples",
    "codegen": "rm -rf src/generatedNoCheck/* && osdk-unstable-typescript generate --outDir src/generatedNoCheck --ontologyPath ontology.json --version 0.1.0 --packageType module --internal --skipPackageJsonUpdate",
    "generateExamples": "generate-examples",
    "lint": "eslint src/index.ts --max-warnings=1",
    "lint:fix": "eslint . --fix",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node"
  },
  "dependencies": {
    "@osdk/client": "workspace:~",
    "@osdk/typescript-docs-example-generator": "workspace:~",
    "@osdk/typescript-sdk-docs": "workspace:~"
  },
  "peerDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~"
  },
  "devDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/cli.cmd.typescript": "workspace:~",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "typescript": "~5.5.4"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
