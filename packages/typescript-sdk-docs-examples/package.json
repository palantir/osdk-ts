{
  "name": "@osdk/typescript-sdk-docs-examples",
  "private": true,
  "version": "0.2.0-beta.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "rm -rf src/generatedNoCheck/* && osdk-unstable-typescript generate --outDir src/generatedNoCheck --ontologyPath ontology.json --version 0.1.0 --packageType module --internal --skipPackageJsonUpdate && rm -rf src/examples/* && generate-examples && pnpm run format",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "format": "npx dprint fmt --config $(find-up dprint.json)",
    "generateExamples": "rm -rf src/examples/* && generate-examples && pnpm run format",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "lint:fix": "eslint src --fix",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {},
  "peerDependencies": {
    "@osdk/client": "workspace:~"
  },
  "devDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/cli.cmd.typescript": "workspace:~",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/typescript-docs-example-generator": "workspace:~",
    "@osdk/typescript-sdk-docs": "workspace:~",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.js",
  "types": "./build/types/index.d.ts",
  "type": "module"
}
