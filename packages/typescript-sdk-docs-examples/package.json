{
  "name": "@osdk/typescript-sdk-docs-examples",
  "private": true,
  "version": "0.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "build": "pnpm run codeGen && pnpm run transpile && pnpm run buildSdkDocs && pnpm run generateExamples",
    "build:dev": "pnpm run codeGen && pnpm run buildSdkDocs && pnpm run generateExamples",
    "build:force": "pnpm run codeGen || true && pnpm run transpile || true && pnpm run buildSdkDocs || true && pnpm run generateExamples || true",
    "clean": "rm -rf dist src/generatedNoCheck examples src/examples",
    "codeGen": "npx --yes @osdk/cli@0.28.1 unstable typescript generate --version 0.1.0 --ontologyPath ./ontology.json --outDir ./src/generatedNoCheck --clean",
    "dev": "tsc --watch",
    "generateExamples": "node generateExamples.mjs",
    "buildSdkDocs": "cd ../typescript-sdk-docs && pnpm run transpileEsm && pnpm run transpileTypes && pnpm run transpileCjs",
    "transpile": "npx tsc || echo 'TypeScript compilation failed but continuing...'",
    "transpile:strict": "npx tsc",
    "lint": "eslint . --max-warnings=1 --ignore-pattern 'src/utils/**'",
    "lint:fix": "eslint . --fix",
    "typecheck:examples": "tsc -p src/examples/tsconfig.examples.json --noEmit",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node"
  },
  "dependencies": {
    "@osdk/client": "^2.4.1",
    "@osdk/typescript-sdk-docs": "workspace:~",
    "handlebars": "^4.7.7"
  },
  "peerDependencies": {
    "@osdk/api": "^2.4.1",
    "@osdk/client": "^2.4.1"
  },
  "devDependencies": {
    "@osdk/api": "^2.4.1",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "ajv": "^8.12.0",
    "eslint": "^8.50.0",
    "typescript": "~5.5.4"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
