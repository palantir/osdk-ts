{
  "name": "@osdk/typescript-sdk-docs-examples",
  "private": true,
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./typescriptOsdkContext": {
      "import": "./dist/typescriptOsdkContext.js",
      "types": "./dist/typescriptOsdkContext.d.ts"
    },
    "./typescriptOsdkExamples": {
      "import": "./dist/typescriptOsdkExamples.js",
      "types": "./dist/typescriptOsdkExamples.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist src/generatedNoCheck examples src/examples",
    "codegen": "rm -rf src/generatedNoCheck/* && osdk-unstable-typescript generate --outDir src/generatedNoCheck --ontologyPath ontology.json --version 0.1.0 --packageType module --internal --skipPackageJsonUpdate && rm -rf src/examples/* && generate-examples --versions 2.0.0,2.1.0,2.4.0 && pnpm run format",
    "format": "npx dprint fmt --config $(find-up dprint.json)",
    "generateExamples": "rm -rf src/examples/* && generate-examples --versions 2.0.0,2.1.0,2.4.0 && pnpm run format",
    "lint": "eslint src/index.ts --max-warnings=1",
    "lint:fix": "eslint src --fix",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@osdk/client": "workspace:~",
    "@osdk/typescript-docs-example-generator": "workspace:~",
    "@osdk/typescript-sdk-docs": "workspace:~"
  },
  "peerDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~"
  },
  "devDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/cli.cmd.typescript": "workspace:~",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "typescript": "~5.5.4"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
