{
  "extends": ["//"],
  "tasks": {
    "codegen": {
      "inputs": ["ontology.json"],
      "outputs": ["src/generatedNoCheck/**/*"],
      "dependsOn": ["@osdk/cli.cmd.typescript#transpile"]
    },
    "generateExamples": {
      "dependsOn": [
        "@osdk/typescript-sdk-docs#codegen",
        "@osdk/typescript-sdk-docs#transpileTypes",
        "@osdk/typescript-docs-example-generator#transpileEsm", 
        "^transpileEsm", 
        "^transpileTypes", 
        "^transpileCjs"
      ]
    },
    "validateExamples": {
      "dependsOn": ["generateExamples"],
      "inputs": ["src/examples/**/*.ts"],
      "cache": false
    },
    "build": {
      "dependsOn": ["codegen", "validateExamples", "transpileEsm"]
    },
    "build:dev": {
      "dependsOn": ["@osdk/typescript-docs-example-generator#transpileEsm"]
    },
    "build:force": {
      "dependsOn": ["@osdk/typescript-docs-example-generator#transpileEsm"]
    }
  }
}