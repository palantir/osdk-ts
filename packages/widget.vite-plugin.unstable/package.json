{
  "name": "@osdk/widget.vite-plugin.unstable",
  "version": "2.0.0-beta.11",
  "description": "A vite plugin that will extract parameter definitions from TS/JS files + entrypoint info into a manifest file to be uploaded to Foundry ",
  "access": "public",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "check-attw": "attw --profile esm-only --pack .",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev": "vite",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "preview": "vite preview",
    "test": "vitest run",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@osdk/foundry-config-json": "workspace:~",
    "@osdk/widget-api.unstable": "workspace:~",
    "escodegen": "^2.1.0",
    "fs-extra": "^11.2.0",
    "parse5": "^7.2.1",
    "picocolors": "^1.1.1",
    "recast": "^0.23.9",
    "sirv": "^3.0.0"
  },
  "peerDependencies": {
    "vite": "^6.0.11"
  },
  "devDependencies": {
    "@blueprintjs/core": "^5.16.0",
    "@blueprintjs/icons": "^5.15.0",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@types/escodegen": "~0.0.10",
    "@types/estree": "^1.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18",
    "react-dom": "^18",
    "ts-expect": "^1.3.0",
    "typescript": "~5.5.4",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vite-plugin"
  ],
  "files": [
    "build/site",
    "build/esm",
    "build/types",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "type": "module"
}
