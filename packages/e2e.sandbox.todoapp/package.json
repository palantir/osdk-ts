{
  "name": "@osdk/e2e.sandbox.todoapp",
  "private": true,
  "version": "3.0.0-beta.8",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "rm -rf src/generatedNoCheck2/* &&  osdk-unstable-typescript generate --outDir src/generatedNoCheck2 --ontologyPath ontology.json --version dev --internal --packageType module",
    "dev": "vite",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "preview": "vite preview",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~",
    "@osdk/oauth": "workspace:~",
    "core-js": "^3.37.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.2.5",
    "tiny-invariant": "^1.3.3"
  },
  "peerDependencies": {
    "@osdk/api": "workspace:~",
    "@osdk/client": "workspace:~"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@osdk/api": "workspace:~",
    "@osdk/cli.cmd.typescript": "workspace:~",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/monorepo.tsup": "workspace:~",
    "@types/core-js": "^2.5.8",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/site",
    "build/esm",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "type": "module"
}
