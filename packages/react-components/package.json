{
  "name": "@osdk/react-components",
  "version": "0.1.0-beta.5",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": "./build/cjs/index.cjs",
      "default": "./build/browser/index.js"
    },
    "./experimental": {
      "browser": "./build/browser/public/experimental.js",
      "import": {
        "types": "./build/types/public/experimental.d.ts",
        "default": "./build/esm/public/experimental.js"
      },
      "require": "./build/cjs/public/experimental.cjs",
      "default": "./build/browser/public/experimental.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "require": "./build/cjs/public/*.cjs",
      "default": "./build/browser/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "attw --pack .",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo && find src -name '*.module.scss.d.ts' -delete",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json) && pnpm lint:scss",
    "lint:scss": "stylelint \"src/**/*.module.scss\"",
    "test": "vitest run",
    "transpileAllDeps": "pnpm --filter @osdk/api --filter @osdk/client --filter @osdk/react run transpileBrowser",
    "transpileBrowser": "pnpm run generate:scss-types && monorepo.tool.transpile -f esm -m normal -t browser",
    "transpileCjs": "pnpm run generate:scss-types && monorepo.tool.transpile -f cjs -m bundle -t node",
    "transpileEsm": "pnpm run generate:scss-types && monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "pnpm run generate:scss-types && monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "pnpm run generate:scss-types && tsc --noEmit --emitDeclarationOnly false",
    "generate:scss-types": "typed-scss-modules src --export-type default"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@blueprintjs/icons": "^6.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.13"
  },
  "peerDependencies": {
    "@osdk/api": "*",
    "@osdk/client": "*",
    "@osdk/react": "*",
    "@types/react": "^17 || ^18 || ^19",
    "classnames": "^2.0.0",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/client": "workspace:*",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/react": "workspace:*",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.22",
    "esbuild-css-modules-plugin": "^3.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "p-defer": "^4.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "react": "^18.3.1",
    "sass": "^1.97.3",
    "stylelint": "^17.1.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "typed-scss-modules": "^8.1.1",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "build/types",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.js",
  "types": "./build/cjs/index.d.cts",
  "type": "module"
}
