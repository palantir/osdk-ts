{
  "name": "@osdk/react-components",
  "version": "0.1.0-beta.6",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": "./build/cjs/index.cjs",
      "default": "./build/browser/index.js"
    },
    "./index.css": {
      "browser": "./build/browser/index.css",
      "default": "./build/esm/index.css"
    },
    "./osdk-react-components.css": {
      "browser": "./build/browser/osdk-react-components.css",
      "import": "./build/esm/osdk-react-components.css",
      "require": "./build/cjs/osdk-react-components.css",
      "default": "./build/esm/osdk-react-components.css"
    },
    "./experimental": {
      "browser": "./build/browser/public/experimental.js",
      "import": {
        "types": "./build/types/public/experimental.d.ts",
        "default": "./build/esm/public/experimental.js"
      },
      "require": "./build/cjs/public/experimental.cjs",
      "default": "./build/browser/public/experimental.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "require": "./build/cjs/public/*.cjs",
      "default": "./build/browser/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "attw --pack .",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run",
    "transpileAllDeps": "pnpm --filter @osdk/api --filter @osdk/client --filter @osdk/react run transpileBrowser",
    "transpileBrowser": "monorepo.tool.transpile -f esm -m normal -t browser && node scripts/compile-css-modules.mjs",
    "transpileCjs": "monorepo.tool.transpile -f cjs -m bundle -t node && node scripts/compile-css-modules.mjs",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node && node scripts/compile-css-modules.mjs",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "compile-css": "node scripts/compile-css-modules.mjs",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@blueprintjs/icons": "^6.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.13"
  },
  "peerDependencies": {
    "@osdk/api": "*",
    "@osdk/client": "*",
    "@osdk/react": "*",
    "@types/react": "^17 || ^18 || ^19",
    "classnames": "^2.0.0",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/client": "workspace:*",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/react": "workspace:*",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.22",
    "eslint-plugin-react-hooks": "^5.2.0",
    "p-defer": "^4.0.1",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "react": "^18.3.1",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "build/types",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.js",
  "types": "./build/cjs/index.d.cts",
  "type": "module",
  "style": "./build/esm/osdk-react-components.css"
}
