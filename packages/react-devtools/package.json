{
  "name": "@osdk/react-devtools",
  "version": "0.0.1",
  "description": "Developer tools and debugging utilities for OSDK React Toolkit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "default": "./build/esm/index.js"
    },
    "./*": {
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "default": "./build/esm/public/*.js"
    }
  },
  "scripts": {
    "build": "rm -rf build && vite build",
    "check-attw": "attw --profile esm-only --pack .",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev": "vite",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "preview": "vite preview",
    "test": "vitest run",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.0.0",
    "@blueprintjs/icons": "^6.0.0",
    "@jridgewell/source-map": "^0.3.11",
    "@visx/axis": "^3.12.0",
    "@visx/glyph": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/text": "^3.12.0",
    "classnames": "2.3.2",
    "zustand": "^4.5.5"
  },
  "peerDependencies": {
    "@osdk/api": "*",
    "@osdk/client": "*",
    "@osdk/react": "*",
    "@types/react": "^17 || ^18 || ^19",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/client": "workspace:*",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@osdk/react": "workspace:*",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "happy-dom": "^16.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.5.4",
    "vite": "^6.3.5",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/site",
    "build/esm",
    "build/types",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "style": "./build/esm/index.css",
  "type": "module"
}
