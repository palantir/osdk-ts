{
  "name": "@osdk/react-docs-components",
  "version": "0.1.0-beta.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/osdk-ts.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": "./build/cjs/index.cjs",
      "default": "./build/browser/index.js"
    },
    "./experimental": {
      "browser": "./build/browser/public/experimental.js",
      "import": {
        "types": "./build/types/public/experimental.d.ts",
        "default": "./build/esm/public/experimental.js"
      },
      "require": "./build/cjs/public/experimental.cjs",
      "default": "./build/browser/public/experimental.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/esm/public/*.js"
      },
      "require": "./build/cjs/public/*.cjs",
      "default": "./build/browser/public/*.js"
    }
  },
  "scripts": {
    "check-attw": "attw --pack .",
    "check-spelling": "cspell --quiet .",
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev": "pnpm --filter @osdk/osdk-react-docs dev",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run",
    "transpileAllDeps": "pnpm --filter @osdk/api --filter @osdk/client --filter @osdk/react run transpileEsm",
    "transpileBrowser": "monorepo.tool.transpile -f esm -m normal -t browser",
    "transpileCjs": "monorepo.tool.transpile -f cjs -m bundle -t node",
    "transpileEsm": "monorepo.tool.transpile -f esm -m normal -t node",
    "transpileTypes": "monorepo.tool.transpile -f esm -m types -t node",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.0.0",
    "@blueprintjs/icons": "^6.0.0",
    "@osdk/docs-spec-core": "catalog:foundry-platform-typescript",
    "@osdk/docs-spec-sdk": "catalog:foundry-platform-typescript",
    "@osdk/foundry.ontologies": "catalog:foundry-platform-typescript",
    "@osdk/typescript-sdk-docs": "workspace:*",
    "classnames": "^2.5.1",
    "mustache": "^4.2.0",
    "prism-react-renderer": "^2.3.0"
  },
  "peerDependencies": {
    "@types/react": "^17 || ^18 || ^19",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@testing-library/react": "^16.3.0",
    "@types/mustache": "^4.2.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint-plugin-react-hooks": "^5.2.0",
    "react": "^18.3.1",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "build/browser",
    "build/types",
    "CHANGELOG.md",
    "package.json",
    "templates",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.js",
  "types": "./build/cjs/index.d.cts",
  "type": "module"
}
