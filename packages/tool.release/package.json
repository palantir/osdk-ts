    "@changesets/config": "^3.1.1",
    "@changesets/git": "^3.0.4",
    "@changesets/pre": "^2.0.2",
    "@changesets/read": "^0.6.5",
    "@changesets/release-utils": "^0.2.5",
    "@manypkg/get-packages": "^1.1.3",
    "@octokit/core": "^6.1.6",
    "@octokit/plugin-throttling": "^9.6.1",
    "adm-zip": "^0.5.16",
    "chalk": "^5.6.2",
    "consola": "^3.4.2",
    "execa": "^9.6.0",
    "latest-version": "^9.0.0",
    "octokit": "^4.1.4",
    "p-filter": "^4.1.0",
    "p-map": "^7.0.3",
    "package-json": "^10.0.1",
    "pkg-versions": "^4.0.0",
    "resolve-from": "^5.0.0",
    "semver": "^7.7.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@changesets/types": "^6.1.0",
    "@osdk/monorepo.api-extractor": "workspace:~",
    "@osdk/monorepo.tsconfig": "workspace:~",
    "@types/adm-zip": "^0.5.7",
    "@types/mdast": "4.0.4",
    "@types/node": "^18.19.124",
    "@types/semver": "^7.7.1",
    "@types/yargs": "^17.0.33",
    "typescript": "~5.5.4",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [],
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
